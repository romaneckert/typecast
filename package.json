{
  "name": "@typecast/core",
  "version": "0.0.4",
  "description": "",
  "scripts": {
    "build": "tsc && cp -rf package.json lib",
    "format": "prettier --write 'src/**/*.ts' 'src/**/*.js'",
    "lint": "tslint -p tsconfig.json",
    "test": "jest --config jestconfig.json",
    "prepare": "npm run build && cd lib",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/romaneckert/typecast-core.git"
  },
  "keywords": [],
  "author": "Roman Eckert",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/romaneckert/typecast-core/issues"
  },
  "homepage": "https://github.com/romaneckert/typecast-core#readme",
  "devDependencies": {
    "@types/jest": "^24.0.13",
    "jest": "^24.8.0",
    "prettier": "^1.17.1",
    "ts-jest": "^24.0.2",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.4.5"
  },
  "dependencies": {
    "typeorm": "0.2.18"
  }
}
