        "@types/jest": "24.0.15",
        "@types/jsonwebtoken": "8.3.2",
        "@types/node": "12.0.8",
        "@types/nodemailer": "^6.2.0",
        "@types/pug": "2.0.4",
        "gulp": "4.0.2",
