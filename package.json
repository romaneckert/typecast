{
    "name": "@typecast/core",
    "version": "0.0.12",
    "description": "",
    "scripts": {
        "start": "npm run build && export NODE_ENV=production && node dist/index.js",
        "dev": "npm run build && export NODE_ENV=development && node dist/index.js",
        "build": "tsc && cp -rf package.json dist && cp -rf LICENSE dist && cp -rf README.md dist",
        "format": "prettier --write 'src/**/*.ts' 'src/**/*.js'",
        "lint": "tslint -p tsconfig.json",
        "test": "jest --config jestconfig.json --runInBand",
        "upload": "npm run lint && npm test && npm run build && cd dist && npm publish",
        "preversion": "npm run lint",
        "version": "npm run format && git add -A src",
        "postversion": "git push"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/romaneckert/typecast-core.git"
    },
    "keywords": [],
    "author": "Roman Eckert",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/romaneckert/typecast-core/issues"
    },
    "homepage": "https://github.com/romaneckert/typecast-core#readme",
    "devDependencies": {
        "@types/compression": "0.0.36",
        "@types/express": "4.17.0",
        "@types/helmet": "0.0.43",
        "@types/jest": "^24.0.14",
        "@types/node": "12.0.8",
        "jest": "^24.8.0",
        "prettier": "^1.18.2",
        "ts-jest": "^24.0.2",
        "tslint": "^5.17.0",
        "tslint-config-prettier": "^1.18.0",
        "typescript": "^3.5.2"
    },
    "dependencies": {
        "compression": "1.7.4",
        "cookie-parser": "1.4.4",
        "express": "4.17.1",
        "helmet": "3.18.0",
        "jsonwebtoken": "8.5.1",
        "mongodb": "3.2.7",
        "nodemailer": "6.2.1",
        "pug": "2.0.3",
        "reflect-metadata": "0.1.13",
        "typeorm": "0.2.18"
    }
}
