{
    "name": "@typecast/core",
    "version": "0.0.61",
    "description": "",
    "scripts": {
        "gulp": "npx gulp",
        "start": "npm run build && cd dist && export NODE_ENV=production && node index.js",
        "dev": "npm run build && cd dist && export NODE_ENV=development && node index.js",
        "build": "rm -rf dist && tsc && cp -rf .env.* dist && cp -rf package.json dist && cp -rf jest.js.json dist && cp -rf LICENSE dist && cp -rf README.md dist && cp -rf view dist && cp -rf public dist && cp -rf locale dist && cp -rf .npmignore dist && cp -rf badges dist",
        "lint": "tslint -p tsconfig.json",
        "test": "npm run build && cd dist && jest --runInBand --config jest.js.json && cd ../ && npm run lint",
        "test:ts": "jest --runInBand --config jest.ts.json && jest-coverage-badges output './badges' && npm run lint",
        "preversion": "npm test",
        "version": "git add -A .",
        "postversion": "git push && npm run build && cd dist && npm publish"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/romaneckert/typecast-core.git"
