    "scripts": {
        "node-ts": "node-ts",
        "gulp": "npm run node-ts gulp",
        "test": "jest",
        "test-watch": "jest --watch",
        "update": "npx npm-update-all"
    },
    "jest": {
        "bail": true,
        "collectCoverage": true,
        "collectCoverageFrom": [
            "<rootDir>/**/*"
        ],
        "coverageReporters": [
            "json-summary",
            "text"
        ],
        "errorOnDeprecated": true,
        "roots": [
            "<rootDir>/src"
        ],
        "testEnvironment": "node",
        "transform": {
            "^.+\\.tsx?$": "ts-jest"
        },
        "verbose": true
    },
