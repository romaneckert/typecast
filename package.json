        "gulp": "^4.0.2",
        "gulp-autoprefixer": "^7.0.0",
        "gulp-sass": "^4.0.2",
        "gulp-strip-css-comments": "^2.0.0",
        "gulp-typescript": "5.0.1",
        "jest": "^24.9.0",
        "jest-coverage-badges": "^1.1.2",
        "node-sass": "^4.12.0",
        "prettier": "^1.18.2",
